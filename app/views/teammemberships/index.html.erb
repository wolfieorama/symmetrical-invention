<p id="notice"><%= notice %></p>

<h1>Listing Teammemberships</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teammemberships.each do |teammembership| %>
      <tr>
        <td><%= teammembership.employee %></td>
        <td><%= teammembership.team %></td>
        <td><%= link_to 'Show', teammembership %></td>
        <td><%= link_to 'Edit', edit_teammembership_path(teammembership) %></td>
        <td><%= link_to 'Destroy', teammembership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teammembership', new_teammembership_path %>
